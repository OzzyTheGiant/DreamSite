{
  "name": "directus-extension-carts",
  "description": "Cart synchronization endpoints for E-Commerce",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-endpoint"
  ],
  "type": "module",
  "directus:extension": {
    "type": "endpoint",
